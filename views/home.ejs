<html>
<head>
<link href="styles/style.css" rel="stylesheet" type="text/css">
</head>
<a href='/logout'>Log out</a>
<body> 
    <div>
    <h1>Time Line <img src='/styles/logo.png' class='logo'></h1>
    <p>Welcome to twitter</p>
</div>
    <div>
    <form method='post' action='/home'>
    <input type='text' name='content' placeholder='What is going on?' required/> <br>
    <input type='submit' value='tweet' class='button'/> 
</form>
</div>
    <div>
            <% for (var i= data.length-1; i>0; i--) { %>
                <div class="tweets">
                <h5 id= "user"> <%= '@' + data[i].username %></h5> 
                    <p><%= data[i].content %></p>
                </div>
            <% } %>
    </body>
</div>
</html>